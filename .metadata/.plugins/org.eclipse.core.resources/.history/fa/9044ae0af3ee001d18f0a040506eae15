package _0510_day18_api_thread.ex2;

public class ThreadTest implements Runnable{

	private int num = 0;
	
	
	@Override
	public void run() {
		
		for(int i = 0; i <=10; i++) {
			
			
			num++;
			System.out.println("-----------------------------------------");
			System.out.println( Thread.currentThread().getName());
			System.out.println(num);
			
			
			try {
				Thread.sleep(1000); // 1ì´ˆ
			} catch (Exception e) {
				e.printStackTrace();
			}
			
			
			
		}
		
		
	}

}
